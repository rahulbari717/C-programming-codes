#include <stdio.h>

void add_number(int , int, int);

int main(){

	add_number(12,5,6);


	printf("Press 'Enter' to exit this application");
	getchar();
	return 0;
}

void add_number(int a, int b, int c){
	// simple function definition without retun type
	int sum;
	sum = a+b+c;
	printf("Sum is ==> %d \n",sum);
}
